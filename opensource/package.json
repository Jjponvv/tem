{
  "name": "tem",
  "displayName": "Tem",
  "description": "Simple templates for filetypes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Formatters"
  ],
  "author": {"name": "Jjponvv", "url": "https://github.com/Jjponvv"},
  "icon": "temlogo.png",
  "activationEvents": ["*"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tem.createFromTemplate", 
        "title": "Create from template"
      },
      {
        "command": "tem.addTemplate", 
        "title": "Add template"
      },
      {
        "command": "tem.editTemplate", 
        "title": "Edit template"
      },
      {
        "command": "tem.deleteTemplate", 
        "title": "Delete template"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
